{
  "name": "@millenniumdb/driver",
  "repository": {
    "type": "git",
    "url": "git://github.com/MillenniumDB/MillenniumDB-driver-javascript.git"
  },
  "bugs": {
    "url": "https://github.com/MillenniumDB/MillenniumDB-driver-javascript/issues"
  },
  "homepage": "https://github.com/MillenniumDB/MillenniumDB-driver-javascript#readme",
  "version": "1.1.2",
  "description": "The official MillenniumDB driver for JavaScript",
  "author": "Vicente Calisto",
  "license": "MIT",
  "type": "module",
  "main": "./lib/index.cjs.js",
  "module": "./lib/index.esm.js",
  "browser": "./lib/index.esm.min.js",
  "types": "./lib/index.d.ts",
  "exports": {
    ".": {
      "import": "./lib/index.esm.js",
      "require": "./lib/index.cjs.js",
      "types": "./lib/index.d.ts"
    }
  },
  "files": [
    "lib"
  ],
  "scripts": {
    "check-types": "tsc --noEmit",
    "check-types:watch": "npm run check-types -- --watch",
    "build": "npm run clean:build && rollup -c && npm run clean:types",
    "test": "jest --verbose",
    "prepublishOnly": "npm run build && npm run test",
    "doc": "npm run clean:doc && typedoc",
    "clean:build": "rimraf lib",
    "clean:types": "rimraf ./lib/types",
    "clean:doc": "rimraf doc"
  },
  "keywords": [
    "millenniumdb",
    "quad model",
    "sparql",
    "rdf",
    "database",
    "driver",
    "db",
    "graph",
    "graph database"
  ],
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.1.4",
    "@types/jest": "^29.5.12",
    "@types/ws": "^8.5.10",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "rimraf": "^5.0.5",
    "rollup": "^4.16.4",
    "rollup-plugin-dts": "^6.2.3",
    "rollup-plugin-polyfill-node": "^0.13.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typedoc": "^0.25.13",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "tslib": "^2.8.1",
    "ws": "^8.17.0"
  }
}
